  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@types/jest": "^26.0.20",
    "@types/text-encoding": "0.0.35",
    "husky": "^5.1.1",
    "np": "^7.4.0",
    "rollup": "^2.39.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
